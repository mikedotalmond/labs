package {	import flash.display.*;	import flash.events.*;	import misc.*;	import wck.*;		/**	 * A button that will remove its parent.	 */	public class RemoveButton extends SimpleButton {			public function RemoveButton() {			addEventListener(MouseEvent.MOUSE_DOWN, handleClick, false, 0, true);		}				public function handleClick(e:Event):void {			e.stopImmediatePropagation();			Util.addChildAtPosOf(Util.findAncestorOfClass(this, wck.World) as DisplayObjectContainer, new FX1(), this.parent);			Util.remove(this.parent);		}	}}